<h1>@ViewBag.Paises.Nombre</h1>
<h1><img src="@ViewBag.Paises.Bandera"></h1>
<ul>
    @foreach (Deportistas item in ViewBag.Deportista)
    {
        <li>@item.Nombre</li>
        <li>@item.Apellido</li>
        <li>@item.FechaDeNacimiento</li>
        <li>
            <img src='@item.Foto'>
        </li>
    }
</ul>